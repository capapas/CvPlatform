{% extends 'CvPlatformUserBundle:Profile:show.html.twig' %}
{% block personal_informations%}
Websites
<div class="well">
    <div class="websites">
        {% for website in websites %}
            <div class="website">
                {{ website.title }}: {{ website.url }}
            </div>
        {% endfor %}
    </div>
</div>
<div class="well">
    <form method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <input type="submit" class="btn btn-primary" />
    </form>
</div>
{% endblock personal_informations%}